<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane fx:id="anchorPane" prefHeight="316.0" prefWidth="636.0">
         <children>
            <Label fx:id="lb1" layoutX="125.0" layoutY="14.0" prefHeight="46.0" prefWidth="497.0" text="Shanghai Railway Path Finder">
               <font>
                  <Font size="31.0" />
               </font>
            </Label>
            <ImageView fx:id="icon" fitHeight="100.0" fitWidth="100.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../railway.png" />
               </image>
            </ImageView>
            <Label fx:id="lb2" layoutX="125.0" layoutY="64.0" prefHeight="30.0" prefWidth="332.0" text="author: zuxinrui" textFill="#8a8989">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <TextField fx:id="inputtf" layoutX="125.0" layoutY="131.0" prefHeight="27.0" prefWidth="332.0" />
            <Button fx:id="calculate" layoutX="475.0" layoutY="131.0" mnemonicParsing="false" onAction="#PathFinder" prefHeight="27.0" prefWidth="126.0" text="规划路径">
               <font>
                  <Font name="Heiti SC Light" size="13.0" />
               </font>
            </Button>
            <TextArea fx:id="outputal" layoutX="125.0" layoutY="171.0" prefHeight="46.0" prefWidth="475.0" />
            <TextField fx:id="outputtime" layoutX="125.0" layoutY="229.0" prefHeight="27.0" prefWidth="176.0" />
            <TextField fx:id="caltime" layoutX="424.0" layoutY="229.0" prefHeight="27.0" prefWidth="176.0" />
            <Label fx:id="lb3" layoutX="44.0" layoutY="234.0" prefHeight="17.0" prefWidth="70.0" text="乘坐时间：">
               <font>
                  <Font name="Heiti SC Light" size="13.0" />
               </font>
            </Label>
            <Label fx:id="lb4" layoutX="338.0" layoutY="234.0" prefHeight="17.0" prefWidth="70.0" text="计算时间：">
               <font>
                  <Font name="Heiti SC Light" size="13.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</GridPane>
